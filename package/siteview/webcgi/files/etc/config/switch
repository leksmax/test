config vlan
    option name 'LAN1'
    option vlan '1'
    option vid '1'
    option ports '2 3 4 5'
    option desc 'VLAN for LAN1'

config vlan
    option name 'WAN1'
    option vlan '2'
    option vid '2'
    option ports '1'
    option desc 'VLAN for WAN1'

config port
    option port '1'
    option pvid '2'
                       
config port
    option port '2'
    option pvid '1'
                       
config port
    option port '3'
    option pvid '1'
                       
config port
    option port '4'
    option pvid '1'
                       
config port
    option port '5'
    option pvid '1'

